export enum CoinType {
    <% coins.forEach((coin) => { -%>
        <%-coin.id%> = <%-coin.code%>,
    <% }) %>
}

export class CoinTypeUtils {
    <% methods.forEach((method) => { -%>
        static <%-method.name%>(coin: CoinType): <%-method.returnType%> {
            switch (coin) {
                <% coins.forEach((coin) => { -%>
                    case CoinType.<%-coin.id%>: <%-method.body(coin)%>;
                <% }) %>
            }
        }
    <% }) %>
}
